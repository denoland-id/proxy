{
  "name": "@denoland-id/proxy",
  "description": "Vercel proxy for denoland.id website and module registry ðŸ”€",
  "version": "0.1.0",
  "repository": "git@github.com:denoland-id/proxy.git",
  "author": "Griko Nibras <griko@denoland.id>",
  "scripts": {
    "format": "yarn format:index && yarn format:api",
    "format:index": "prettier --write \"*.{js,json,md}\"",
    "format:api": "prettier --write \"api/**/*.{js,ts}\"",
    "contributors:add": "all-contributors add",
    "contributors:generate": "all-contributors generate",
    "prepare": "yarn format"
  },
  "devDependencies": {
    "@types/node-fetch": "^2.5.7",
    "@vercel/node": "^1.6.1",
    "all-contributors-cli": "^6.15.0",
    "husky": "^4.2.5",
    "node-fetch": "^2.6.0",
    "prettier": "^2.0.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn contributors:generate && yarn prepare"
    }
  },
  "prettier": {
    "semi": true,
    "singleQuote": false,
    "trailingComma": "all"
  },
  "license": "MIT"
}
